{
  "name": "cnpm",
  "version": "5.2.0",
  "description": "cnpm: npm client for cnpmjs.org",
  "main": "bin/cnpm",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "mocha -t 120000 test/*.test.js",
    "lint": "jshint .",
    "ci": "npm run lint && npm run test",
    "autod": "autod",
    "pkg": "gulp"
  },
  "preferGlobal": true,
  "bin": {
    "cnpm": "bin/cnpm"
  },
  "pkg": {
    "scripts": [
      "bin/cnpm-check",
      "bin/cnpm-doc",
      "bin/cnpm-search",
      "bin/cnpm-sync",
      "bin/cnpm-user",
      "bin/cnpm-web"
    ]
  },
  "dependencies": {
    "auto-correct": "^1.0.0",
    "bagpipe": "^0.3.5",
    "colors": "^1.1.2",
    "commander": "~2.10.0",
    "cross-spawn": "~0.2.8",
    "debug": "^2.2.0",
    "giturl": "^1.0.0",
    "ini": "^1.3.4",
    "npm": "^5.0.3",
    "npm-request": "^1.0.0",
    "npminstall": "^3.0.0",
    "open": "^0.0.5",
    "urllib": "^2.17.0"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "coffee": "^3.3.0",
    "egg-ci": "^1.1.0",
    "fs-extra": "0",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.2",
    "gulp-rename": "^1.2.2",
    "gulp-tar": "^2.1.0",
    "gulp-zip": "^4.1.0",
    "jshint": "2",
    "mocha": "3",
    "rimraf": "^2.6.2",
    "should": "5"
  },
  "homepage": "https://github.com/cnpm/cnpm",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/cnpm.git",
    "web": "https://github.com/cnpm/cnpm"
  },
  "bugs": {
    "url": "https://github.com/cnpm/cnpm/issues",
    "email": "fengmk2@gmail.com"
  },
  "keywords": [
    "cnpm",
    "npm",
    "npminstall"
  ],
  "engines": {
    "node": ">= 4.0.0"
  },
  "ci": {
    "version": "4, 6, 8",
    "command": {
      "travis": "ci",
      "appveyor": "ci"
    }
  },
  "author": "fengmk2 <fengmk2@gmail.com> (https://fengmk2.com)",
  "license": "MIT"
}
